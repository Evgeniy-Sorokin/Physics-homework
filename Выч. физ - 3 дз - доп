    "print(A)\n",
    "u, s, d = np.linalg.svd(A)\n",
    "o = np.diagflat(s)\n",
    "l = np.array([1,0])\n",
    "F = d.T@np.append((np.linalg.inv(o)@u.T@l), np.zeros(8))\n",
    "print(A@F.T)"
