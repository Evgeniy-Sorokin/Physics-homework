    "with np.load('/Users/20450/Desktop/data7.npz') as data:\n",
    "    x_1, y_1 = data['x1'], data['y1']\n",
    "    \n",
    "x_0 = np.linspace(min(x_1), max(x_1), 10000)\n",
    "fun_lagrange = scipy.interpolate.lagrange(x_1, y_1)\n",
    "plt.subplot(3, 1, 1)\n",
    "plt.plot(x_0, fun_lagrange(x_0), color = 'k')\n",
    "plt.scatter(x_1, y_1, color = 'r')\n",
    "plt.ylim(min(y_1) - 0.1, max(y_1) + 0.1)\n",
    "\n",
    "fun_phic = scipy.interpolate.PchipInterpolator(x_1, y_1)\n",
    "plt.subplot(3, 1, 3)\n",
    "plt.plot(x_0, fun_phic(x_0), color = 'k')\n",
    "plt.scatter(x_1, y_1, color = 'r')\n",
    "\n",
    "fun_spline = scipy.interpolate.CubicSpline(x_1, y_1)\n",
    "plt.subplot(3, 1, 2)\n",
    "plt.plot(x_0, fun_spline(x_0), color = 'k')\n",
    "plt.scatter(x_1, y_1, color = 'r')\n",
    "\n",
    "plt.show()"
